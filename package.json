{
  "name": "jsonresume-theme-microdata",
  "version": "0.22.0",
  "description": "A flat JSON Resume theme, compatible with the latest resume schema",
  "keywords": [
    "resume",
    "json",
    "theme",
    "flat",
    "curriculum-vitae",
    "cv",
    "microdata",
    "schema.org"
  ],
  "homepage": "https://jsonresume-theme-even.rbrd.in/",
  "bugs": {
    "url": "https://github.com/rbardini/jsonresume-theme-microdata/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rbardini/jsonresume-theme-microdata"
  },
  "license": "MIT",
  "author": "Rafael Bardini",
  "type": "module",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.js"
  },
  "main": "./dist/index.cjs",
  "unpkg": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "source": "index.js",
  "typings": "./dist/index.d.ts",
  "bin": {
    "jsonresume-theme-microdata": "bin/cli.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "format": "prettier .",
    "lint": "eslint --ignore-pattern .gitignore .",
    "prepare": "husky && json2ts node_modules/resume-schema/schema.json schema.d.ts",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "type-check": "tsc"
  },
  "dependencies": {
    "feather-icons": "^4.29.1",
    "micromark": "^4.0.0",
    "striptags": "^3.2.0"
  },
  "devDependencies": {
    "@codemirror/commands": "6.5.0",
    "@codemirror/lang-json": "6.0.1",
    "@codemirror/theme-one-dark": "6.1.2",
    "@codemirror/view": "6.26.3",
    "@types/feather-icons": "4.29.4",
    "@vitest/coverage-v8": "1.5.2",
    "codemirror": "6.0.1",
    "debounce": "2.0.0",
    "eslint": "9.1.1",
    "html-validate": "8.18.2",
    "husky": "9.0.11",
    "json-schema-to-typescript": "14.0.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "prettier-plugin-organize-imports": "3.2.4",
    "prettier-plugin-packagejson": "2.5.0",
    "resume-schema": "1.0.0",
    "typescript": "5.4.5",
    "vite": "5.2.10",
    "vite-plugin-dts": "3.9.0",
    "vite-plugin-static-copy": "1.0.3",
    "vitest": "1.5.2"
  }
}
