{
  "name": "jsonresume-theme-microdata",
  "version": "0.22.0",
  "description": "A flat JSON Resume theme, infused with microdata using the schema.org vocabulary",
  "keywords": [
    "resume",
    "json",
    "theme",
    "flat",
    "curriculum-vitae",
    "cv",
    "microdata",
    "schema.org"
  ],
  "homepage": "https://github.com/scottnath/jsonresume-theme-microdata",
  "bugs": {
    "url": "https://github.com/scottnath/jsonresume-theme-microdata/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottnath/jsonresume-theme-microdata.git"
  },
  "license": "MIT",
  "author": {
    "name": "scottnath"
  },
  "type": "module",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.js"
  },
  "main": "./dist/index.cjs",
  "unpkg": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "source": "index.js",
  "typings": "./dist/index.d.ts",
  "bin": {
    "jsonresume-theme-microdata": "bin/cli.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "postbuild": "cp ./sample.resume.json ./dist",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=chpt_0f59236527eec54",
    "dev": "vite",
    "format": "prettier .",
    "lint": "eslint --ignore-pattern .gitignore ./components",
    "prepare": "husky && json2ts node_modules/resume-schema/schema.json schema.d.ts",
    "prepublishOnly": "npm run build",
    "prerelease": "npm run build",
    "release": "semantic-release --debug=true",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test-storybook": "test-storybook --coverage --verbose",
    "type-check": "tsc"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "dependencies": {
    "@cucumber/microdata": "^2.1.0",
    "feather-icons": "^4.29.1",
    "micromark": "^4.0.0",
    "micromark-extension-gfm": "^3.0.0",
    "resumed": "^3.0.1",
    "striptags": "^3.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@codemirror/commands": "6.5.0",
    "@codemirror/lang-json": "6.0.1",
    "@codemirror/theme-one-dark": "6.1.2",
    "@codemirror/view": "6.26.3",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/blocks": "^8.0.9",
    "@storybook/test": "^8.0.9",
    "@storybook/test-runner": "^0.17.0",
    "@storybook/web-components": "^8.0.9",
    "@storybook/web-components-vite": "^8.0.9",
    "@types/feather-icons": "4.29.4",
    "@vitest/coverage-v8": "1.5.2",
    "chromatic": "^11.3.0",
    "codemirror": "6.0.1",
    "debounce": "2.0.0",
    "eslint": "9.1.1",
    "eslint-plugin-storybook": "^0.8.0",
    "html-validate": "8.18.2",
    "husky": "9.0.11",
    "json-schema-to-typescript": "14.0.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "prettier-plugin-organize-imports": "3.2.4",
    "prettier-plugin-packagejson": "2.5.0",
    "resume-schema": "1.0.0",
    "storybook": "^8.0.9",
    "storydocker-utilities": "^0.0.20",
    "typescript": "5.4.5",
    "vite": "5.2.10",
    "vite-plugin-dts": "3.9.0",
    "vite-plugin-static-copy": "1.0.3",
    "vitest": "1.5.2"
  }
}
